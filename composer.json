{
    "name": "smoggert/swagger-generator",
    "description": "Generates swagger documentation based on your laravel API.",
    "keywords": ["laravel","api","open-api","swagger"],
    "license": "MIT",
    "authors": [
        {
            "name": "Peter De Bleye",
            "email": "peter.debleye@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel":{
            "providers":[
                "Smoggert\\SwaggerGenerator\\SwaggerGeneratorServiceProvider"
            ]
        } 
    },
    "autoload": {
        "psr-4": {
            "Smoggert\\SwaggerGenerator\\": "src"
        }
    },
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0|^10.0|^11.0"
    }
}
